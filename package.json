{
  "name": "gatsby-awesome-pagination-ts",
  "version": "1.0.0",
  "description": "An opinionated, more awesome approach to pagination in Gatsby, improved with TypeScript",
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prepack": "yarn run build",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "lint": "eslint ./src ./__tests__ --ext ts --ext tsx",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldenpathtechnologies/gatsby-awesome-pagination.git"
  },
  "bugs": {
    "url": "https://github.com/goldenpathtechnologies/gatsby-awesome-pagination/issues"
  },
  "homepage": "https://github.com/goldenpathtechnologies/gatsby-awesome-pagination#readme",
  "keywords": [
    "gatsby",
    "pagination"
  ],
  "author": "Daryl Wright",
  "contributors": [
    "Callum Macdonald"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "gatsby": ">=2.0.0",
    "lodash": "^4.17.21",
    "react": ">=16.0.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/exec",
      [ "@semantic-release/git", {
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }]
    ],
    "preset": "conventionalcommits",
    "parserOpts": {
      "noteKeywords": [
        "BREAKING CHANGE",
        "BREAKING CHANGES",
        "BREAKING"
      ]
    },
    "writerOpts": {
      "commitsSort": [
        "subject",
        "scope"
      ]
    }
  }
}
